package org.n52.sos.datasource;
import java.util.Map;
import java.util.Properties;
import java.util.Set;
import org.n52.sos.config.SettingDefinition;
import org.n52.sos.config.settings.StringSettingDefinition;
import org.n52.sos.ds.Datasource;
import com.google.common.collect.Sets;

public class datas implements Datasource {

private static final String DIALECT = "scidb";
	private static final String KEY_NETCDF_URI = "uri.path.scidb";
	private static final String DEFAULT_NETCDF_URI = "/scidb";

	protected final StringSettingDefinition createNetcdfUri() {
		return new StringSettingDefinition().setGroup(BASE_GROUP).setOrder(1)
				.setKey(KEY_NETCDF_URI).setTitle("Path to scidb-Directory")
				.setDefaultValue(DEFAULT_NETCDF_URI);
	}

	@Override
	public final String getDialectName() {
		return DIALECT;
	}

	

}

